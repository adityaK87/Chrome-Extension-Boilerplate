{
    "manifest_version": 3,
    "name": "Color-Picker",
    "version": "1.0.0",
    "description": "creating extension using webpack and react",
    "icons": {
        "16": "icon-16.png",
        "48": "icon-48.png",
        "128": "icon-128.png"
    },
    "action": {
        "default_title": "chrome-extension",
        "default_popup": "popup.html"
    },
    "background": {
        "service_worker": "background.js"
    },
    "permissions": [
        "bookmarks",
        "scripting"
    ]
}